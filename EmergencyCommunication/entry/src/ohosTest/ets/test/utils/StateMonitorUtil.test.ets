import { describe, beforeAll, beforeEach, afterEach, afterAll, it, expect } from '@ohos/hypium';
import { checkRadioOn } from '../../../../main/ets/utils/StateMonitorUtil';

export default function StateMonitorUtilTest() {
  describe('StateMonitorUtilTest', () => {
    it('checkRadioOn', 0, () => {
      checkRadioOn((accountId) => {
        let res = accountId === 0 ? true : (accountId === 1 ? true : false)
        expect(res).assertEqual(true);
      })
    })
  })
}