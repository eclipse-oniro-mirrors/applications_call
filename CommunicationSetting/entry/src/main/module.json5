{
  "module": {
    "name": "entry",
    "metadata": [
      {
        "name": "action.settings.search.path",
        "value": "searchConfig/searchPage.json"
      }
    ],
    "srcEntry": "./ets/application/MyAbilityStage.ets",
    "type": "entry",
    "description": "$string:module_desc",
    "mainElement": "CommunicationSettingAbility",
    "deviceTypes": [
      "default",
      "tablet"
    ],
    "deliveryWithInstall": true,
    "installationFree": false,
    "pages": "$profile:main_pages",
    "requestPermissions": [
      {
        "name": "ohos.permission.VIBRATE"
      },
      {
        "name": "ohos.permission.GET_INSTALLED_BUNDLE_LIST",
        "reason": "$string:reason_get_installed_app_list",
        "usedScene": {
          "abilities": [
            "CommunicationSettingAbility"
          ],
          "when": "inuse"
        }
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO_PRIVILEGED",
        "reason": "$string:reason_get_bundle_info_privileged"
      },
      {
        "name": "ohos.permission.GET_BUNDLE_RESOURCES",
        "reason": "$string:reason_get_bundle_resources",
      },
      {
        "name": "ohos.permission.GET_TELEPHONY_STATE",
        "reason": "$string:reason_get_telephony_state",
      },
      {
        "name": "ohos.permission.MANAGE_NET_STRATEGY",
        "reason": "$string:reason_manage_net_strategy",
      },
      {
        "name": "ohos.permission.GET_NETWORK_STATS",
        "reason": "$string:reason_get_network_stats",
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO",
        "reason": "$string:reason_get_bundle_info",
      },
      {
        "name": "ohos.permission.SET_TELEPHONY_STATE",
        "reason": "$string:reason_set_telephony_state",
      },
      {
        "name": "ohos.permission.MANAGE_SETTINGS",
        "reason": "$string:reason_manage_settings",
      },
      {
        "name": "ohos.permission.GET_NETWORK_INFO",
        "reason": "$string:GET_NETWORK_INFO"
      },
      {
        "name": "ohos.permission.ACCESS_SYSTEM_SETTINGS",
      },
      {
        "name": "ohos.permission.START_ABILITIES_FROM_BACKGROUND"
      }
    ],
    "extensionAbilities": [
      {
        "name": "CommunicationSettingAbility",
        "srcEntry": "./ets/entryability/CommunicationSettingAbility.ets",
        "description": "$string:CommunicationSettingAbility_desc",
        "label": "$string:CommunicationSettingAbility_label",
        "icon": "$media:app_icon",
        "type": "sys/commonUI",
        "exported": true,
        "metadata": [
          {
            "name": "UIExtensionAbilityLaunchTypeTemp",
            "value": "singleton"
          }
        ],
        "permissions": ["ohos.permission.GET_INSTALLED_BUNDLE_LIST"]
      },
      {
        "name": "ConnectionExtAbility",
        "srcEntry": "./ets/entryability/ConnectionExtAbility.ets",
        "description": "WindowExtension",
        "icon": "$media:appIcon",
        "type": "sys/commonUI",
        "exported": true,
        "metadata": [
          {
            "name": "UIExtensionAbilityLaunchTypeTemp",
            "value": "singleton"
          }
        ],
        "permissions": [
          "ohos.permission.GET_NETWORK_STATS"
        ]
      },
      {
        "name": "UIExtensionProvider",
        "srcEntry": "./ets/entryability/NetAccessPolicyUIExtension.ets",
        "description": "UIExtensionProvider",
        "type": "sysDialog/common",
        "icon": "$media:appIcon"
      },
      {
        "name": "DisableMobileDataDialogAbility",
        "srcEntry": "./ets/entryability/DisableMobileDataDialogAbility.ets",
        "description": "WindowExtension",
        "icon": "$media:appIcon",
        "type": "sysDialog/common",
        "exported": true,
        "metadata": [
          {
            "name": "UIExtensionAbilityLaunchTypeTemp",
            "value": "singleton"
          }
        ],
        "permissions": [
          "ohos.permission.SET_TELEPHONY_STATE"
        ]
      },
      {
        "name": "StaticSubscriber",
        "srcEntry": "./ets/service/StaticSubscriber.ets",
        "description": "StaticSubscriber",
        "icon": "$media:appIcon",
        "label": "$string:CommunicationSettingAbility_label",
        "type": "staticSubscriber",
        "metadata": [
          {
            "name": "ohos.extension.staticSubscriber",
            "resource": "$profile:staticsubscriber"
          }
        ]
      },
      {
        "name": "SettingsSearchServiceExtAbility",
        "srcEntrance": "./ets/service/SettingsSearchServiceExtAbility.ets",
        "type": "service",
        "visible": false,
        "description": "SettingsSearchServiceExtAbility",
        "permissions": []
      }
    ]
  }
}