{
  "app": {
    "bundleName": "com.ohos.callui",
    "vendor": "ohos",
    "version": {
      "code": 1000000,
      "name": "1.0.0"
    }
  },
  "deviceConfig": {},
  "module": {
    "package": "com.ohos.callui",
    "name": ".MyApplication",
    "mainAbility": "com.ohos.callui.MainAbility",
    "srcPath": "",
    "deviceType": [
      "phone",
      "tablet"
    ],
    "distro": {
      "deliveryWithInstall": true,
      "moduleName": "callui",
      "moduleType": "entry",
      "installationFree": false
    },
    "abilities": [
      {
        "skills": [
          {
            "entities": [
              "entity.system.home"
            ],
            "actions": [
              "action.system.home"
            ]
          }
        ],
        "orientation": "unspecified",
        "visible": true,
        "srcPath": "default",
        "name": ".MainAbility",
        "srcLanguage": "ets",
        "icon": "$media:icon",
        "description": "JS_Empty Ability",
        "formsEnabled": false,
        "label": "$string:entry_MainAbility",
        "type": "page",
        "launchType": "singleton"
      },
      {
        "name": ".ServiceAbility",
        "icon": "$media:icon",
        "description": "service",
        "type": "service",
        "srcPath": "ServiceAbility",
        "srcLanguage": "ets",
        "visible": true
      }
    ],
    "js": [
      {
        "mode": {
          "syntax": "ets",
          "type": "pageAbility"
        },
        "pages": [
          "pages/index"
        ],
        "name": ".MainAbility",
        "window": {
          "designWidth": 720,
          "autoDesignWidth": false
        }
      }
    ],
        "reqPermissions" : [
      {
        "name" : "ohos.permission.READ_CONTACTS"
      },
      {
        "name" : "ohos.permission.SEND_MESSAGES"
      },
      {
        "name": "ohos.permission.GET_BUNDLE_INFO_PRIVILEGED"
      }
    ]
  }
}